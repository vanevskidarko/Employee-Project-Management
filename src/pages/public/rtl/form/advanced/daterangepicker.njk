{% extends "components/template.njk" %}

{% set direction = "rtl" %}
{% set page_title = "Date Range Picker" %}
{% set breadcrumb = [
  { text: "Dashboard", link: "rtl/index.html" | url, icon: '<i data-feather="home"></i>' },
  { text: "Form" },
  { text: "Advanced" },
  { text: "Date range picker", link: "rtl/form/advanced/daterangepicker.html" | url }
] %}
{% set js_links = [
  "assets/app/form/daterangepicker.js" | url
] %}

{% block content %}
  <div class="container-fluid">
    <div class="row">
      <div class="col-12">

        <!-- BEGIN Portlet -->
        <div class="portlet">
          <div class="portlet-header portlet-header-bordered">
            <h3 class="portlet-title">Date range picker</h3>
          </div>
          <div class="portlet-body">
            <p><strong>Date Range Picker</strong> is javascript component for choosing date ranges, dates and times. For more info please visit the plugin's <a href="http://www.daterangepicker.com" target="_blank">homepage</a>.</p>
            <hr>

            <!-- BEGIN Portlet -->
            <div class="form-group row">
              <label class="col-sm-4 col-lg-3 col-form-label text-muted text-sm-left">Basic demo</label>
              <div class="col-sm-5 col-lg-4">

                <!-- BEGIN Input Group -->
                <div class="input-group">
                  <input class="form-control" type="text" id="daterangepicker-1" value="02/06/2020 - 02/20/2020">
                  <div class="input-group-append">
                    <span class="input-group-text">
                      <i class="fa fa-calendar"></i>
                    </span>
                  </div>
                </div>
                <!-- END Input Group -->

              </div>
            </div>
            <!-- END Portlet -->

            <!-- BEGIN Portlet -->
            <div class="form-group row">
              <label class="col-sm-4 col-lg-3 col-form-label text-muted text-sm-left">Date and time picker</label>
              <div class="col-sm-5 col-lg-4">

                <!-- BEGIN Input Group -->
                <div class="input-group">
                  <input class="form-control" type="text" id="daterangepicker-2" value="02/06/2020 - 02/20/2020">
                  <div class="input-group-append">
                    <span class="input-group-text">
                      <i class="fa fa-calendar"></i>
                    </span>
                  </div>
                </div>
                <!-- END Input Group -->

              </div>
            </div>
            <!-- END Portlet -->

            <!-- BEGIN Portlet -->
            <div class="form-group row">
              <label class="col-sm-4 col-lg-3 col-form-label text-muted text-sm-left">Single picker</label>
              <div class="col-sm-5 col-lg-4">

                <!-- BEGIN Input Group -->
                <div class="input-group">
                  <input class="form-control" type="text" id="daterangepicker-3" value="02/15/2020">
                  <div class="input-group-append">
                    <span class="input-group-text">
                      <i class="fa fa-calendar"></i>
                    </span>
                  </div>
                </div>
                <!-- END Input Group -->

              </div>
            </div>
            <!-- END Portlet -->

            <!-- BEGIN Portlet -->
            <div class="form-group row">
              <label class="col-sm-4 col-lg-3 col-form-label text-muted text-sm-left">Predefined picker</label>
              <div class="col-sm-5 col-lg-4">

                <!-- BEGIN Input Group -->
                <div class="input-group">
                  <input class="form-control" type="text" id="daterangepicker-4" value="02/06/2020 - 02/20/2020">
                  <div class="input-group-append">
                    <span class="input-group-text">
                      <i class="fa fa-calendar"></i>
                    </span>
                  </div>
                </div>
                <!-- END Input Group -->

                <p class="mb-0 mt-2">Replace default calendar with predefined time list</p>
              </div>
            </div>
            <!-- END Portlet -->

            <!-- BEGIN Portlet -->
            <div class="form-group row">
              <label class="col-sm-4 col-lg-3 col-form-label text-muted text-sm-left">Disabled date</label>
              <div class="col-sm-5 col-lg-4">

                <!-- BEGIN Input Group -->
                <div class="input-group">
                  <input class="form-control" type="text" id="daterangepicker-5" autocomplete="off">
                  <div class="input-group-append">
                    <span class="input-group-text">
                      <i class="fa fa-calendar"></i>
                    </span>
                  </div>
                </div>
                <!-- END Input Group -->

              </div>
            </div>
            <!-- END Portlet -->

            <!-- BEGIN Portlet -->
            <div class="form-group row">
              <label class="col-sm-4 col-lg-3 col-form-label text-muted text-sm-left">Complex form</label>
              <div class="col-sm-5 col-lg-4">

                <!-- BEGIN Input Group -->
                <div class="input-group">
                  <input class="form-control" type="text" id="daterangepicker-6" value="02/06/2020 - 02/20/2020">
                  <div class="input-group-append">
                    <span class="input-group-text">
                      <i class="fa fa-calendar"></i>
                    </span>
                  </div>
                </div>
                <!-- END Input Group -->

              </div>
            </div>
            <!-- END Portlet -->

            <!-- BEGIN Portlet -->
            <div class="form-group row mb-0">
              <label class="col-sm-4 col-lg-3 col-form-label text-muted text-sm-left">Locale</label>
              <div class="col-sm-5 col-lg-4">

                <!-- BEGIN Input Group -->
                <div class="input-group">
                  <input class="form-control" type="text" id="daterangepicker-7" value="02/06/2020 - 02/20/2020">
                  <div class="input-group-append">
                    <span class="input-group-text">
                      <i class="fa fa-calendar"></i>
                    </span>
                  </div>
                </div>
                <!-- END Input Group -->

              </div>
            </div>
            <!-- END Portlet -->

          </div>
        </div>
        <!-- END Portlet -->

      </div>
    </div>
  </div>
{% endblock %}
